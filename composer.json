{
  "name": "neolikotsi/payfast",
  "authors": [
    {
      "name": "Warren Hansen",
      "email": "warren@billow.co.za"
    },
    {
      "name": "Neo Likotsi",
      "email": "info@bengen.co.za"
    }
  ],
  "description": "Laravel 5.4+|6+|7+ package for processing ITN payments through payfast.co.za",
  "keywords": ["package", "laravel", "payfast"],
  "license": "MIT",
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/neolikotsi/show-me-the-money"
    }
],
  "require": {
    "illuminate/support": "^5.4|^6.0|^7.0",
    "illuminate/http": "^5.4|^6.0|^7.0",
    "neolikotsi/show-me-the-money": "^0.2.4"
  },
  "autoload": {
    "psr-4": {
      "NeoLikotsi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NeoLikotsi\\Payfast\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^4.3",
    "phpunit/phpunit": "^8.5|^9.1",
    "orchestra/testbench": "^3.0|^4.6|^5.0|^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NeoLikotsi\\PayfastServiceProvider"
      ]
    }
  }
}
